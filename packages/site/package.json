{
  "name": "site",
  "version": "1.0.0-rc.2",
  "private": true,
  "scripts": {
    "allow-scripts": "yarn workspace root allow-scripts",
    "prebuild": "rimraf build",
    "build": "DISABLE_ESLINT_PLUGIN=true craco build",
    "build-storybook": "build-storybook -s public -c storybook",
    "check-types": "tsc --noemit",
    "eject": "craco eject",
    "generate": "npm run generate:icons && npm run generate:images",
    "generate:icons": "node script/iconGenerator.js src/ui/assets/icons/ src/ui/common/icons/",
    "generate:images": "node script/imagesGenerator.js src/ui/assets/images/",
    "lint": "eslint . --ext .ts,.json --max-warnings=0 && yarn check-types",
    "lint:fix": "eslint . --ext .ts,.json --max-warnings=0 --fix && yarn check-types",
    "start": "npm run generate && craco start",
    "storybook": "start-storybook -p 6006 -s public -c storybook",
    "test": "jest --maxWorkers=30%",
    "test:ci": "jest --runInBand",
    "test:coverage": "yarn test --coverage",
    "test:coverage:data-access": "yarn test:data-access --coverage",
    "test:coverage:domain": "yarn test:domain --coverage",
    "test:coverage:ui": "yarn test:ui --coverage",
    "test:data-access": "yarn test ./test/data-access",
    "test:debug": "jest --no-cache --runInBand",
    "test:debug:data-access": "jest ./test/data-access --no-cache --runInBand",
    "test:debug:domain": "jest ./test/domain --no-cache --runInBand",
    "test:debug:ui": "jest ./test/ui --no-cache --runInBand",
    "test:domain": "yarn test ./test/domain",
    "test:ui": "yarn test ./test/ui",
    "test:watch": "yarn test --watch",
    "test:watch:data-access": "yarn test:data-access --watch",
    "test:watch:domain": "yarn test:domain --watch",
    "test:watch:ui": "yarn test:ui --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "resolutions": {
    "@types/react": "^18.2.79"
  },
  "dependencies": {
    "@craco/craco": "^7.0.0",
    "@metamask/providers": "^17.0.0",
    "@peersyst/react-components": "^3.9.31",
    "@peersyst/react-components-core": "^1.7.19",
    "@peersyst/react-hooks": "^2.2.15",
    "@peersyst/react-utils": "^2.5.0",
    "@tanstack/react-query": "^5.40.0",
    "@transak/transak-sdk": "^3.1.2",
    "babel-plugin-styled-components": "^2.0.7",
    "clsx": "^1.2.1",
    "decimal.js": "^10.4.3",
    "i18next": "^23.11.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.2",
    "react-qrcode-logo": "^3.0.0",
    "react-router-dom": "6.8.0",
    "react-scripts": "^5.0.1",
    "react18-json-view": "^0.2.8",
    "styled-components": "^5.3.6",
    "text-encoding": "^0.7.0",
    "typescript": "^4.7.4",
    "web-vitals": "^3.1.1",
    "xrpl": "2.13.0",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@cypress/code-coverage": "3.10.0",
    "@cypress/instrument-cra": "^1.4.0",
    "@metamask/eslint-config": "^12.2.0",
    "@metamask/eslint-config-browser": "^12.1.0",
    "@metamask/eslint-config-jest": "^12.1.0",
    "@metamask/eslint-config-nodejs": "^12.1.0",
    "@metamask/eslint-config-typescript": "^12.1.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@swisstype/essential": "^0.1.1",
    "@swisstype/react": "^0.0.2",
    "@swisstype/string": "^0.0.5",
    "@testing-library/cypress": "9.0.0",
    "@testing-library/dom": "8.20.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.0.1",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "babel-jest": "^29.4.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "customize-cra": "^1.0.0",
    "customize-cra-react-refresh": "^1.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsdoc": "^41.1.2",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "openapi-typescript": "^6.1.0",
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "^2.8.3",
    "prop-types": "^15.8.1",
    "react-error-overlay": "6.0.11",
    "ts-jest": "^29.0.5",
    "webpack": "^5.75.0"
  }
}
